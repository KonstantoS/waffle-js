{
  "name": "waffle-js",
  "version": "0.0.1",
  "description": "WaffleJS is an auxiliary application for commenting any object on your web pages for customers while running on staging.",
  "main": "index.js",
  "repository": "gi",
  "author": "Kostiantyn Levytskyi <le.konstantos@gmail.com>",
  "license": "MPL-2.0",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.development.js",
    "start:development": "cross-env PROJECT_ENV=development npm run start",
    "start:staging": "cross-env PROJECT_ENV=staging npm run start",
    "start:production": "cross-env PROJECT_ENV=production npm run start",
    "build": "webpack --config config/webpack.config.production.js",
    "build:development": "cross-env BABEL_ENV=production PROJECT_ENV=development npm run build",
    "build:staging": "cross-env BABEL_ENV=production PROJECT_ENV=staging npm run build",
    "build:production": "cross-env BABEL_ENV=production PROJECT_ENV=production npm run build",
    "clean": "rimraf dist",
    "release:development": "npm run clean && npm run build:development",
    "release:staging": "npm run clean && npm run build:staging",
    "release:production": "npm run clean && npm run build:production",
    "analyzer": "cross-env BABEL_ENV=production PROJECT_ENV=production webpack --config config/webpack.config.production.js",
    "eslint": "eslint config src webpack --ext .js --ext .jsx",
    "prettier": "prettier '{config,src,webpack}/**/*.{js,jsx}' --write",
    "stylelint": "stylelint 'src/**/*.scss'",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "@medv/finder": "^1.0.3",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-3": "^7.0.0-beta.51",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "autoprefixer": "^8.6.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "svg-sprite-loader": "^3.8.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
